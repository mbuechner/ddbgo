{
    "name": "ddbkultur/ddbgo",
    "description": "DDBgo composer project for Deutsche Digitale Bibliothek <https://www.deutsche-digitale-bibliothek.de/>",
    "type": "project",
    "license": "Apache-2.0",
    "authors": [{
            "name": "Michael BÃ¼chner <m.buechner@dnb.de>",
            "role": "Developer"
        }
    ],
    "repositories": [{
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }, {
            "type": "package",
            "package": {
                "name": "ddbgolib/fontawesome",
                "version": "5.15.3",
                "type": "drupal-library",
                "dist": {
                    "url": "https://use.fontawesome.com/releases/v5.15.3/fontawesome-free-5.15.3-web.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.11"
                }
            }
        }, {
            "type": "package",
            "package": {
                "name": "ddbgolib/datetimepicker",
                "version": "2.5.20",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/xdan/datetimepicker/archive/2.5.20.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.11"
                }
            }
        }, {
            "type": "package",
            "package": {
                "name": "ddbgolib/fontawesome-iconpicker",
                "version": "3.2.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/farbelous/fontawesome-iconpicker/archive/3.2.0.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.11"
                }
            }
        }, {
            "type": "package",
            "package": {
                "name": "ddbgolib/jquery-simple-color",
                "version": "1.2.3",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/recurser/jquery-simple-color/archive/v1.2.3.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.11"
                }
            }
        }, {
            "type": "package",
            "package": {
                "name": "ddbgolib/select2",
                "version": "4.0.13",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/select2/select2/archive/4.0.13.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.11"
                }
            }
        }, {
            "type": "package",
            "package": {
                "name": "ddbgolib/jquery-ui-touch-punch",
                "version": "1.0.x-dev",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/furf/jquery-ui-touch-punch/archive/refs/heads/master.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.11"
                }
            }
        }, {
            "type": "package",
            "package": {
                "name": "ddbgolib/spectrum",
                "version": "1.8.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/bgrins/spectrum/archive/1.8.0.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.11"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.11",
        "cweagans/composer-patches": "^1.7",
        "ddbgolib/datetimepicker": "^2.5",
        "ddbgolib/fontawesome": "^5.15.3",
        "ddbgolib/fontawesome-iconpicker": "^3.2",
        "ddbgolib/jquery-simple-color": "^1.2.3",
        "ddbgolib/jquery-ui-touch-punch": "1.0.x-dev",
        "ddbgolib/select2": "^4.0",
        "ddbgolib/spectrum": "^1.8",
        "drupal/admin_toolbar": "^3.3",
        "drupal/adminimal_admin_toolbar": "^1.11",
        "drupal/adminimal_theme": "^1.6",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/color": "^1.0",
        "drupal/color-color": "^1.0",
        "drupal/color_field": "^3.0",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/content_lock": "^2.2",
        "drupal/core": "^9.0",
        "drupal/core-composer-scaffold": "^9.2",
        "drupal/custom_formatters": "^4.0@beta",
        "drupal/date_popup": "^2.0",
        "drupal/diff": "^1.0",
        "drupal/empty_fields": "^1.0",
        "drupal/entity_reference_integrity": "^1.2",
        "drupal/entity_reference_revisions": "^1.9",
        "drupal/exclude_node_title": "^1.2",
        "drupal/facets": "^3.0@beta",
        "drupal/field_defaults": "^2.0@alpha",
        "drupal/field_group": "^3.x-dev",
        "drupal/field_validation": "^1.0@beta",
        "drupal/flag": "^4.0@beta",
        "drupal/flood_control": "^2.3",
        "drupal/fontawesome": "^2.19",
        "drupal/fontawesome_menu_icons": "^2.0",
        "drupal/form_options_attributes": "^2.0",
        "drupal/health_check": "^3.0",
        "drupal/hook_post_action": "^1.1",
        "drupal/jquery_ui_accordion": "^2.0",
        "drupal/jsonapi_extras": "^3.19",
        "drupal/key_auth": "^2.0",
        "drupal/libraries": "^4.0",
        "drupal/menu_block": "^1.7",
        "drupal/metatag": "^2.0",
        "drupal/node_view_permissions": "^1.5",
        "drupal/noreqnewpass": "^1.2",
        "drupal/paragraphs": "^1.12",
        "drupal/redis": "^1.5",
        "drupal/role_delegation": "^1.1",
        "drupal/search_api": "^1.19",
        "drupal/select2": "^1.13",
        "drupal/semanticviews": "^2.3",
        "drupal/seven": "^1.0@alpha",
        "drupal/smtp": "^1.1",
        "drupal/taxonomy_unique": "^2.3",
        "drupal/term_merge": "^2.0@beta",
        "drupal/token": "^1.9",
        "drupal/toolbar_menu": "^3.0",
        "drupal/toolbar_menu_clean": "^1.3",
        "drupal/transliterate_filenames": "^2.0",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/unique_field_ajax": "^2.1",
        "drupal/upgrade_status": "^4.0@alpha",
        "drupal/userprotect": "^1.1",
        "drupal/views_data_export": "^1.0",
        "drupal/viewsreference": "^2.0@beta",
        "drush/drush": "^10.5",
        "natxet/cssmin": "^3.0",
        "symfony/string": "^5.3",
        "vlucas/phpdotenv": "^4.2",
        "webflo/drupal-finder": "^1.2",
        "webmozart/path-util": "^2.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.1",
            "ext-gd": "1"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true
        }
    },
    "autoload": {
        "classmap": ["scripts/composer/ScriptHandler.php"],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": ["DrupalProject\\composer\\ScriptHandler::checkComposerVersion"],
        "pre-update-cmd": ["DrupalProject\\composer\\ScriptHandler::checkComposerVersion"],
        "post-install-cmd": ["DrupalProject\\composer\\ScriptHandler::createRequiredFiles"],
        "post-update-cmd": ["DrupalProject\\composer\\ScriptHandler::createRequiredFiles"]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "web/libraries/datetimepicker": ["ddbgolib/datetimepicker"],
            "web/libraries/fontawesome": ["ddbgolib/fontawesome"],
            "web/libraries/fontawesome-iconpicker": ["ddbgolib/fontawesome-iconpicker"],
            "web/libraries/jquery-simple-color": ["ddbgolib/jquery-simple-color"],
            "web/libraries/select2": ["ddbgolib/select2"],
            "web/libraries/spectrum": ["ddbgolib/spectrum"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            },
            "file-mapping": {
                "[web-root]/README.md": false
            }
        },
        "composer-exit-on-patch-failure": true,
        "patches": {
            "drupal/select2": {
                "Changed tokenSeparator in Select2 module": "patches/Changed_tokenSeparator_in_Select2_module.patch"
            },
            "drupal/unique_field_ajax": {
                "Fixing bug with warning message at Unique Field Ajax": "patches/Bugfix_for_Warning_Message_in_unique_field_ajax_module.patch"
            }
        }
    }
}
